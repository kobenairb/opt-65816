{
    "version": 4,
    "env": {
        "65816-optimizer": [
            "${workspaceFolder}/src/**"
        ]
    },
    "configurations": [
        {
            "name": "65816-optimizer",
            "compilerArgs": [
                "-Wall",
                "-Wextra",
                "-O2",
                "-g"
            ],
            "includePath": [
                "${65816-optimizer}"
            ],
            "browse": {
                "limitSymbolsToIncludedHeaders": true,
                "databaseFilename": "",
                "path": [
                    "${65816-optimizer}"
                ]
            },
            "configurationProvider": "ms-vscode.makefile-tools"
        }
    ]
}